{
  "name": "Beta CSS Gradiant Color Generator",
  "description": "Gradiant color generator for CSS",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },

  "permissions": ["activeTab"],

  "action": {
    "default_icon": "icon_16.png",
    "default_popup": "popup.html"
  }
}
